{
    "version": 2,
    "name": "vercel",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "server.js"
        },
        {
            "src": "/products",
            "dest": "index.js"
        },
        {
            "src": "/products/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}